{
  "name": "relax",
  "version": "1.0.0",
  "description": "Social Network",
  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "push": "bash ./scripts/push.sh",
    "start": "if [ ! -d \"db\" ]; then initdb db; fi; sleep 3s; pg_ctl -D db -l db/log.txt start; node ./dist/src/server.js 2>&1 > /dev/null & echo 'Node server binded to http://localhost:3000'",
    "stop": "pg_ctl -D db stop; pkill -f 'node ./dist/src/server.js'",
    "showPgLogs": "cat db/log.txt | less",
    "deletePgLogs": "rm db/log.txt",
    "delete": "yes | rm ./db -r",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stringmanolo/relax.git"
  },
  "author": "StringManolo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stringmanolo/relax/issues"
  },
  "homepage": "https://github.com/stringmanolo/relax#readme",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/node": "^16.11.10",
    "@types/pg": "^8.6.1",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "typescript": "^4.5.2"
  }
}
